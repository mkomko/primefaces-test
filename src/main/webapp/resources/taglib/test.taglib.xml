<?xml version="1.0" encoding="UTF-8"?>

<facelet-taglib xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                version="2.2"
                xmlns="http://xmlns.jcp.org/xml/ns/javaee"
                xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facelettaglibrary_2_2.xsd">

    <namespace>http://at.test.test/jsf-taglib</namespace>

    <tag>
        <tag-name>panel</tag-name>
        <source>panel.xhtml</source>
        <attribute>
            <name>id</name>
            <type>java.lang.String</type>
            <required>true</required>
        </attribute>
        <attribute>
            <name>header</name>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <name>color</name>
            <type>java.lang.String</type>
        </attribute>
    </tag>

    <tag>
    <tag-name>icon</tag-name>
    <source>icon.xhtml</source>
    <attribute>
        <name>styleClass</name>
        <required>true</required>
        <type>java.lang.String</type>
    </attribute>
</tag>

</facelet-taglib>
